<h1>診断結果を見る</h1>  
<% @career_anchor.each do |career_anchor| %> 
 <% if current_user.id != career_anchor.user_id %>
    <%= "診断結果はありません" %>
   <% break %>
   <% elsif current_user.id == career_anchor.user_id %> 
    <li>
       <%= l career_anchor.created_at %>
       <%= link_to "#{current_user.name}さんの診断結果", career_anchor_path(career_anchor.id), method: :get %>
    </li> 
  <% end %>
<% end %>